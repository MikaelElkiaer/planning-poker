.container-fluid
    .row
        .col-sm-4.col-sm-offset-4
            .panel.panel-default
                .panel-heading
                    h3.panel-title
                        | Game
                        span.pull-right {{ GameId }}
                .panel-body
                    .span(*ngFor='let p of PlayersList')
                        i.fa.fa-user
                        | &nbsp;{{p.User.UserName}}