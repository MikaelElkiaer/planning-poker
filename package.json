{
  "name": "planning-poker",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run env:prod && npm run build:prod && npm run start:prod",
    "dev": "npm run env:dev && tsc && npm run start:dev",
    "start:dev": "tsc -w & nodemon ./server/bin/www.js --ext js --watch ./server --watch ./shared --delay 3",
    "start:prod": "node ./server/bin/www.js",
    "env:dev": "cp ./app/config/environment.dev.ts ./app/config/environment.ts",
    "env:prod": "cp ./app/config/environment.prod.ts ./app/config/environment.ts",
    "build:prod": "tsc && npm run minify",
    "minify": "./minify.sh",
    "reindex": "./reindex.sh"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mikaelec/planning-poker/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "@angular/router": "^3.4.4",
    "@angular/upgrade": "^2.4.4",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.17",
    "@types/compression": "^0.0.33",
    "@types/core-js": "^0.9.34",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.5",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.27",
    "angular2-toaster": "^2.0.0",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "crypto": "^0.0.3",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "inversify": "^3.0.0",
    "pug": "^2.0.0-beta11",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0",
    "socket.io": "^1.7.2",
    "systemjs": "^0.20.9",
    "typescript": "2.2.0",
    "uglifyjs": "^2.4.10",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
